(define (gcd a b)
  (if (= b 0)
      a
      (gcd b (remainder a b))))

; normal order => 18 reductions
(gcd 206 40)
(if (= 40 0))
(gcd 40 (r 206 40))
(if (= (r 206 40) 0))
(if (= 6 0))
(gcd (r 206 40) (r 40 (r 206 40)))
(if (= (r 40 (r 206 40)) 0))
(if (= (r 40 6) 0))
(if (= 4 0))
(gcd (r 40 (r 206 40)) (r (r 206 40) (r 40 (r 206 40))))
(if (= (r (r 206 40) (r 40 (r 206 40))) 0)
(if (= (r (r 206 40) (r 40 6)) 0)
(if (= (r 6 (r 40 6)) 0)
(if (= (r 6 4) 0)
(if (= 2 0))
(gcd (r (r 206 40) (r 40 (r 206 40))) (r (r 40 (r 206 40)) (r (r 206 40) (r 40 (r 206 40))))))
(if (= (r (r 40 6) (r (r 206 40) (r 40 (r 206 40)))) 0))
(if (= (r 4 (r (r 206 40) (r 40 (r 206 40)))) 0))
(if (= (r 4 (r (r 206 40) (r 40 6))) 0))
(if (= (r 4 (r (r 206 40) 4)) 0))
(if (= (r 4 (r 6 4)) 0))
(if (= (r 4 2) 0))
(if (= 0 0))
(r (r 206 40) (r 40 (r 206 40)))
(r (r 206 40) (r 40 6))
(r (r 206 40) 4)
(r 6 4)
2

; applic order => 4 reductions
(gcd 206 40)
(if (= 40 0))
(gcd 40 (r 206 40))
(gcd 40 6)
(if (= 6 0))
(gcd 6 (r 40 6))
(gcd 6 4)
(if (= 4 0))
(gcd 4 (r 6 4))
(gcd 4 2)
(if (= 2 0))
(gcd 2 (r 4 2))
(gcd 2 0)
(if (= 0 0))
2